{
  "project_short_name":              {exception":"", "type":"STRING",   "description":"Project name abbreviation, eg TARGET-AML"},
  "case_barcode":                    {exception":"", "type":"STRING",   "description":"Original TARGET case barcode, eg TARGET-20-PASCGR"},
  "sample_barcode":                  {exception":"", "type":"STRING",   "description":"TARGET sample barcode, eg TARGET-20-PASWAT-09A"},
  "aliquot_barcode":                 {exception":"", "type":"STRING",   "description":"TARGET aliquot barcode, eg TARGET-20-PAJLIP-01A-01R"},
  "primary_site":                    {exception":"", "type":"STRING",   "description":"Anatomical site of the cancer under investigation or review"},
  "miRNA_id":                        {exception":"", "type":"STRING",   "description":"Unique miRNA id (aka symbol), eg hsa-mir-21 -- relevant reference information can be found in the isb-cgc:genome_reference dataset in the tables miRBase_v21 and miRBase_v21_hsa_gff3"},
  "chromosome":                      {exception":"", "type":"STRING",   "description":"Chromosome, possible values: chr1-22, and chrX"},
  "start_pos":                       {exception":"", "type":"INTEGER",  "description":"1-based start chromosome position of isoform"},
  "end_pos":                         {exception":"", "type":"INTEGER",  "description":"1-based end chromosome position of isoform"},
  "strand":                          {exception":"", "type":"STRING",   "description":"Either + or - to denote whether read mapped to the sense (+) or anti-sense (-) strand"},
  "read_count":                      {exception":"", "type":"INTEGER",  "description":"Number of reads that were mapped to this mirna_id"},
  "reads_per_million_miRNA_mapped":  {exception":"", "type":"FLOAT",    "description":"Read count normalized by total reads mapped divided by 1 million"},
  "cross_mapped":                    {exception":"", "type":"STRING",   "description":"A short isomiR read may map exactly to mature strands whose sequences are similar but not identical, when the read sequence does not capture the bases that distinguish these miRNAs (e.g. hsa-mir-30a at 6q13 and hsa-mir-30e at 1p34.2, which differ at position 18). We report such a <line_feed> read as cross-mapped, and we increment the read count for each MIMAT that it mapped to. Either Y or N -- fewer than 2% are Y"},
  "miRNA_transcript":                {exception":"", "type":"STRING",   "description":"The type of transcript -- either mature, precursor, unannotated, or stemloop (if this value is not mature, then the mirna_accession field will be null)"},
  "miRNA_accession":                 {exception":"", "type":"STRING",   "description":"The mature miRNA accession id (eg MIMAT0000062) -- note that this can also be null, in the event that this read does not map to a mature portion of the miRNA"},
  "case_gdc_id":                     {exception":"", "type":"STRING",   "description":"GDC unique identifier for this case (corresponds to the case_barcode) -- this can be used to access more information from the GDC data portal like this: https://portal.gdc.cancer.gov/files/c21b332c-06c6-4403-9032-f91c8f407ba13"},
  "sample_gdc_id":                   {exception":"", "type":"STRING",   "description":"Unique GDC identifier for this sample (corresponds to the sample_barcode), eg a1ec9279-c1a6-4e58-97ed-9ec1f36187c5 -- this can be used to access more information from the GDC data portal"},
  "aliquot_gdc_id":                  {exception":"", "type":"STRING",   "description":"Unique GDC identifier for this aliquot (corresponds to the aliquot_barcode), eg 7fbfdb3e-1fd2-4206-8d2e-7f68e4a15844 -- this can be used to access more information from the GDC data portal"},
  "file_gdc_id":                     {exception":"", "type":"STRING",   "description":"Unique GDC identifier for this file (corresponds to the file_barcode) -- this can be used to access more information from the GDC data portal like this: https://portal.gdc.cancer.gov/files/c21b332c-06c6-4403-9032-f91c8f407ba13"}
}
