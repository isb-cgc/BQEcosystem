  CLIN_SIG:{"exception":"","type":"","description":"Clinical significance of variant from dbSNP"},
  SOMATIC:{"exception":"","type":"","description":"Somatic status of each ID reported under Existing_variation (0, 1, or null)"},
  PUBMED:{"exception":"","type":"","description":"Pubmed ID(s) of publications that cite existing variant"},
  TRANSCRIPTION_FACTORS:{"exception":"","type":"","description":"todo new field"},
  MOTIF_NAME:{"exception":"","type":"","description":"The source and identifier of a transcription factor binding profile aligned at this position"},
  MOTIF_POS:{"exception":"","type":"","description":"The relative position of the variation in the aligned TFBP"},
  HIGH_INF_POS:{"exception":"","type":"","description":"A flag indicating if the variant falls in a high information position of a transcription factor binding profile (TFBP) (Y, N, or null)"},
  MOTIF_SCORE_CHANGE:{"exception":"","type":"","description":"The difference in motif score of the reference and variant sequences for the TFBP"},
  miRNA:{"exception":"","type":"","description":"todo new field"},
  IMPACT:{"exception":"","type":"","description":"The impact modifier for the consequence type"},
  PICK:{"exception":"","type":"","description":"Indicates if this block of consequence data was picked by VEP's   pick feature (1 or null)"},
  VARIANT_CLASS:{"exception":"","type":"","description":"Sequence Ontology variant class"},
  TSL:{"exception":"","type":"","description":"Transcript support level, which is based on independent RNA analyses"},
  HGVS_OFFSET:{"exception":"","type":"","description":"Indicates by how many bases the HGVS notations for this variant have been shifted"},
  PHENO:{"exception":"","type":"","description":"Indicates if existing variant is associated with a phenotype, disease or trait (0, 1, or null)"},
  GENE_PHENO:{"exception":"","type":"","description":"Indicates if gene that the variant maps to is associated with a phenotype, disease or trait (0, 1, or null)"},
  CONTEXT:{"exception":"","type":"","description":"The reference allele per VCF specs, and its five flanking base pairs"},
