{
  project_short_name:{"exception":"","type":"","description":"Project name abbreviation; the program name appended with a project name abbreviation; eg. TCGA-OV, etc."},
  case_barcode:{"exception":"","type":"","description":"Original case barcode, eg TCGA-DX-A8BN"},
  primary_site:{"exception":"","type":"","description":"Anatomical site of the cancer under investigation or review"},
  Hugo_Symbol:{"exception":"","type":"","description":"HUGO symbol for the gene (HUGO symbols are always in all caps). Unknown is used for regions that do not correspond to a gene"},
  Entrez_Gene_Id:{"exception":"","type":"","description":"Entrez gene ID (an integer). 0 is used for regions that do not correspond to a gene region or Ensembl ID"},
  Center:{"exception":"","type":"","description":"One or more genome sequencing center reporting the variant"},
  NCBI_Build:{"exception":"","type":"","description":"The reference genome used for the alignment (GRCh38)"},
  Chromosome:{"exception":"","type":"","description":"Chromosome, possible values: chr1-22, and chrX"},
  Start_Position:{"exception":"","type":"","description":"Lowest numeric position of the reported variant on the genomic reference sequence. Mutation start coordinate"},
  End_Position:{"exception":"","type":"","description":"Highest numeric genomic position of the reported variant on the genomic reference sequence. Mutation end coordinate"},
  Strand:{"exception":"","type":"","description":"Either + or - to denote whether read mapped to the sense (+) or anti-sense (-) strand"},
  Variant_Classification:{"exception":"","type":"","description":"Translational effect of variant allele"},
  Variant_Type:{"exception":"","type":"","description":"Type of mutation. TNP (tri-nucleotide polymorphism) is analogous to DNP (di-nucleotide polymorphism) but for three consecutive nucleotides. ONP (oligo-nucleotide polymorphism) is analogous to TNP but for consecutive runs of four or more (SNP, DNP, TNP, ONP, INS, DEL, or Consolidated)"},
  Reference_Allele:{"exception":"","type":"","description":"The plus strand reference allele at this position. Includes the deleted sequence for a deletion or - for an insertion"},
  Tumor_Seq_Allele1:{"exception":"","type":"","description":"Primary data genotype for tumor sequencing (discovery) allele 1. A - symbol for a deletion represents a variant. A - symbol for an insertion represents wild-type allele. Novel inserted sequence for insertion does not include flanking reference bases"},
  Tumor_Seq_Allele2:{"exception":"","type":"","description":"Primary data genotype for tumor sequencing (discovery) allele 2. A - symbol for a deletion represents a variant. A - symbol for an insertion represents wild-type allele. Novel inserted sequence for insertion does not include flanking reference bases"},
  dbSNP_RS:{"exception":"","type":"","description":"The rs-IDs from the   dbSNP database, novel if not found in any database used, or null if there is no dbSNP record, but it is found in other databases"},
  dbSNP_Val_Status:{"exception":"","type":"","description":"The dbSNP validation status is reported as a semicolon-separated list of statuses. The union of all rs-IDs is taken when there are multiple"},
  Tumor_Aliquot_Barcode:{"exception":"","type":"","description":"Aliquot barcode for the tumor sample"},
  Matched_Norm_Aliquot_Barcode:{"exception":"","type":"","description":"Aliquot barcode for the matched normal sample"},
  Match_Norm_Seq_Allele1:{"exception":"","type":"","description":"Primary data genotype. Matched normal sequencing allele 1. A - symbol for a deletion represents a variant. A - symbol for an insertion represents wild-type allele. Novel inserted sequence for insertion does not include flanking reference bases (cleared in somatic MAF)"},
  Match_Norm_Seq_Allele2:{"exception":"","type":"","description":"Matched normal sequencing allele 2"},
  Tumor_Validation_Allele1:{"exception":"","type":"","description":"Secondary data from orthogonal technology. Tumor genotyping (validation) for allele 1. A - symbol for a deletion represents a variant. A - symbol for an insertion represents wild-type allele. Novel inserted sequence for insertion does not include flanking reference bases"},
  Tumor_Validation_Allele2:{"exception":"","type":"","description":"Secondary data from orthogonal technology. Tumor genotyping (validation) for allele 2"},
  Match_Norm_Validation_Allele1:{"exception":"","type":"","description":"Secondary data from orthogonal technology. Matched normal genotyping (validation) for allele 1. A - symbol for a deletion represents a variant. A - symbol for an insertion represents wild-type allele. Novel inserted sequence for insertion does not include flanking reference bases (cleared in somatic MAF)"},
  Match_Norm_Validation_Allele2:{"exception":"","type":"","description":"Secondary data from orthogonal technology. Matched normal genotyping (validation) for allele 2 (cleared in somatic MAF)"},
  Verification_Status:{"exception":"","type":"","description":"Second pass results from independent attempt using same methods as primary data source. Generally reserved for 3730 Sanger Sequencing"},
  Validation_Status:{"exception":"","type":"","description":"Second pass results from orthogonal technology"},
  Mutation_Status:{"exception":"","type":"","description":"An assessment of the mutation as somatic, germline, LOH, post transcriptional modification, unknown, or none. The values allowed in this field are constrained by the value in the Validation_Status field"},
  Sequencing_Phase:{"exception":"","type":"","description":"TCGA sequencing phase (if applicable). Phase should change under any circumstance that the targets under consideration change"},
  Sequence_Source:{"exception":"","type":"","description":"Molecular assay type used to produce the analytes used for sequencing. Allowed values are a subset of the SRA 1.5 library_strategy field values. This subset matches those used at CGHub"},
  Validation_Method:{"exception":"","type":"","description":"The assay platforms used for the validation call"},
  Score:{"exception":"","type":"","description":"Not in use"},
  BAM_File:{"exception":"","type":"","description":"Not in use"},
  Sequencer:{"exception":"","type":"","description":"Instrument used to produce primary sequence data"},
  Tumor_Aliquot_UUID:{"exception":"","type":"","description":"Unique GDC identifier for tumor aliquot (10189 unique)"},
  Matched_Norm_Aliquot_UUID:{"exception":"","type":"","description":"Unique GDC identifier for normal aliquot (10189 unique)"},
  HGVSc:{"exception":"","type":"","description":"The coding sequence of the variant in HGVS recommended format"},
  HGVSp:{"exception":"","type":"","description":"The protein sequence of the variant in HGVS recommended format. p.= signifies no change in the protein"},
  HGVSp_Short:{"exception":"","type":"","description":"Same as the HGVSp column, but using 1-letter amino-acid codes"},
  Transcript_ID:{"exception":"","type":"","description":"Ensembl ID of the transcript affected by the variant"},
  Exon_Number:{"exception":"","type":"","description":"The exon number (out of total number)"},
  t_depth:{"exception":"","type":"","description":"Read depth across this locus in tumor BAM"},
  t_ref_count:{"exception":"","type":"","description":"Read depth supporting the reference allele in tumor BAM"},
  t_alt_count:{"exception":"","type":"","description":"Read depth supporting the variant allele in tumor BAM"},
  n_depth:{"exception":"","type":"","description":"Read depth across this locus in normal BAM"},
  n_ref_count:{"exception":"","type":"","description":"Read depth supporting the reference allele in normal BAM (cleared in somatic MAF)"},
  n_alt_count:{"exception":"","type":"","description":"Read depth supporting the variant allele in normal BAM (cleared in somatic MAF)"},
  all_effects:{"exception":"","type":"","description":"A semicolon delimited list of all possible variant effects, sorted by priority ([Symbol,Consequence,HGVSp_Short,Transcript_ID,RefSeq,HGVSc,Impact,Canonical,Sift,PolyPhen,Strand])"},
  Allele:{"exception":"","type":"","description":"The variant allele used to calculate the consequence"},
  Gene:{"exception":"","type":"","description":"The gene symbol. In this table, gene symbol is gene name e.g. ACADVL"},
  Feature:{"exception":"","type":"","description":"Stable Ensembl ID of feature (transcript, regulatory, motif)"},
  Feature_type:{"exception":"","type":"","description":"Type of feature. Currently one of Transcript, RegulatoryFeature, MotifFeature (or blank)"},
  One_Consequence:{"exception":"","type":"","description":"The single consequence of the canonical transcript in  sequence ontology terms, eg missense_variant"},
  Consequence:{"exception":"","type":"","description":"Consequence type of this variant; sequence ontology terms"},
  cDNA_position:{"exception":"","type":"","description":"Relative position of base pair in the cDNA sequence as a fraction. A - symbol is displayed as the numerator if the variant does not appear in cDNA"},
  CDS_position:{"exception":"","type":"","description":"Relative position of base pair in coding sequence. A - symbol is displayed as the numerator if the variant does not appear in coding sequence"},
  Protein_position:{"exception":"","type":"","description":"Relative position of affected amino acid in protein. A - symbol is displayed as the numerator if the variant does not appear in coding sequence"},
  Amino_acids:{"exception":"","type":"","description":"Amino acid substitution caused by the mutation. Only given if the variation affects the protein-coding sequence"},
  Codons:{"exception":"","type":"","description":"The alternative codons with the variant base in upper case"},
  Existing_variation:{"exception":"","type":"","description":"Known identifier of existing variation"},
  DISTANCE:{"exception":"","type":"","description":"Shortest distance from the variant to transcript"},
  TRANSCRIPT_STRAND:{"exception":"","type":"","description":"The DNA strand (1 or -1) on which the transcript/feature lies"},
  SYMBOL:{"exception":"","type":"","description":"Eg TP53, LRP1B, etc (same as Hugo_Symbol field except blank instead of Unknown"},
  SYMBOL_SOURCE:{"exception":"","type":"","description":"The source of the gene symbol, usually HGNC, rarely blank, other sources include Uniprot_gn, EntrezGene, etc"},
  HGNC_ID:{"exception":"","type":"","description":"Gene identifier from the HUGO Gene Nomenclature Committee if applicable"},
  BIOTYPE:{"exception":"","type":"","description":"Biotype of transcript"},
  CANONICAL:{"exception":"","type":"","description":"A flag (YES) indicating that the VEP-based canonical transcript, the longest translation, was used for this gene. If not, the value is null"},
  CCDS:{"exception":"","type":"","description":"The  CCDS identifier for this transcript, where applicable"},
  ENSP:{"exception":"","type":"","description":"The Ensembl protein identifier of the affected transcript"},
  SWISSPROT:{"exception":"","type":"","description":"UniProtKB/Swiss-Prot accession"},
  TREMBL:{"exception":"","type":"","description":"UniProtKB/TrEMBL identifier of protein product"},
  UNIPARC:{"exception":"","type":"","description":"UniParc identifier of protein product"},
  UNIPROT_ISOFORM:{"exception":"","type":"","description":"todo new field"},
  RefSeq:{"exception":"","type":"","description":"RefSeq identifier for this transcript"},
  MANE:{"exception":"","type":"","description":"todo new field"},
  APPRIS:{"exception":"","type":"","description":"todo new field"},
  FLAGS:{"exception":"","type":"","description":"todo new field"},
  SIFT:{"exception":"","type":"","description":"The SIFT prediction and/or score, with both given as prediction (score)"},
  PolyPhen:{"exception":"","type":"","description":"The PolyPhen prediction and/or score"},
  EXON:{"exception":"","type":"","description":"The exon number (out of total number)"},
  INTRON:{"exception":"","type":"","description":"The intron number (out of total number)"},
  DOMAINS:{"exception":"","type":"","description":"The source and identifier of any overlapping protein domains"},
